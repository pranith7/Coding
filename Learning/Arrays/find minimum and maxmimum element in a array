#include<iostream>
using namespace std;
int main() {
pair<long long, long long> getMinMax(long long a[] , int n);

    long long int mine=1000000000001;
    long long int max=0;

    for(int i=0; i<n; i++)
    {
        if(a[i]>maxe)
        {
            maxe=a[i];
        }
    }
    for (int i=0; i<n; i++)
    {
        if(a[i]<mine)
        {
            mine=a[i];
        }
        
    }

    pair<long long, long long>ans;
    ans.first=mine;
    ans.second=max;

    return ans;
}
